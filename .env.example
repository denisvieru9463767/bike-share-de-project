# =========================
# Airflow Configuration
# =========================
AIRFLOW_UID=50000
_AIRFLOW_WWW_USER_USERNAME=airflow
_AIRFLOW_WWW_USER_PASSWORD=airflow

# =========================
# Snowflake Connection
# =========================
# Used by Airflow for data warehouse operations
SNOWFLAKE_ACCOUNT=your_account.region
SNOWFLAKE_USER=your_service_user
SNOWFLAKE_PRIVATE_KEY_PATH=/path/to/private/key.p8
SNOWFLAKE_PRIVATE_KEY_PASSPHRASE=your_passphrase
SNOWFLAKE_DATABASE=BIKE_SHARE
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_SCHEMA=RAW
SNOWFLAKE_ROLE=your_role

# =========================
# Staging Database (PostgreSQL)
# =========================
STAGING_DB_HOST=staging-db
STAGING_DB_PORT=5432
STAGING_DB_USER=capstone_user
STAGING_DB_PASSWORD=capstone_password
STAGING_DB_NAME=staging_data

# =========================
# dbt Configuration
# =========================
# Note: dbt profile should be configured in ~/.dbt/profiles.yml
# See dbt/README.md for setup instructions

# =========================
# Optional: Superset
# =========================
SUPERSET_SECRET_KEY=your_secret_key_here
SUPERSET_ADMIN_USERNAME=admin
SUPERSET_ADMIN_PASSWORD=admin
